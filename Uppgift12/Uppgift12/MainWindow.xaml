<Window x:Class="Uppgift12.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Uppgift12"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid x:Name="acount">
        <ProgressBar x:Name="Prgbar" HorizontalAlignment="Left" Height="48" Margin="208,0,0,0" VerticalAlignment="Center" Width="321" ValueChanged="Prgbar_ValueChanged"/>
        <Label x:Name="lblotur" Content="Mindre otur" HorizontalAlignment="Left" Margin="112,0,0,0" VerticalAlignment="Center"/>
        <Label x:Name="lbloturb" Content="Mer Otur" HorizontalAlignment="Left" Margin="566,0,0,0" VerticalAlignment="Center"/>
        <Label x:Name="lblvalue" Content="Label" HorizontalAlignment="Left" Margin="350,134,0,0" VerticalAlignment="Top"/>
        <Button x:Name="Btncalc" Content="Starta" HorizontalAlignment="Left" Margin="445,272,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.219,-2.572" Click="Button_Click"/>
        <TextBox x:Name="txtbox" HorizontalAlignment="Left" Margin="219,274,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" RenderTransformOrigin="1.256,1.949" TextChanged="txtbox_TextChanged"/>
        <Label HorizontalAlignment="Left" Margin="149,355,0,0" VerticalAlignment="Top"/>
        <Label x:Name="count" Content="Label" HorizontalAlignment="Left" Margin="208,344,0,0" VerticalAlignment="Top"/>
        <Label x:Name="counttwo" Content="Label" HorizontalAlignment="Left" Margin="411,355,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnreset" Content="Börja om" HorizontalAlignment="Left" Margin="440,307,0,0" VerticalAlignment="Top" Click="btnreset_Click"/>

    </Grid>
</Window>
