<Window x:Class="Uppgift17.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Uppgift17"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="txtBox" HorizontalAlignment="Left" Margin="166,133,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="188,183,0,0" VerticalAlignment="Top" Click="Button_Click" Command="{Binding MyCommand}"/>
        <Label x:Name="lblres" Content="Label" HorizontalAlignment="Left" Margin="415,164,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblcount" Content="Label" HorizontalAlignment="Left" Margin="166,249,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
